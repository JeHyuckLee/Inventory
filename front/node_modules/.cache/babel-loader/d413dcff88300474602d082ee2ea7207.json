{"ast":null,"code":"import MemebrRegister from '@/components/MemberRegister.vue';\nimport axios from 'axios';\nimport { mapActions, mapMutations, mapState } from 'vuex';\nexport default {\n  name: \"coopRegister\",\n  computed: { ...mapState(['memberModal', 'memebrList'])\n  },\n\n  created() {\n    this.getMemberList();\n  },\n\n  methods: { ...mapMutations(['closeMemberModal', 'openMemberModal']),\n    ...mapActions(['getMemberList']),\n\n    deleteMember(id) {\n      axios.delete(`/api/member/delete/${id}`).then(() => {\n        this.getMemberList();\n      }).catch(err => {\n        console.error(err);\n      });\n    }\n\n  },\n\n  data() {\n    return {\n      memberList: []\n    };\n  },\n\n  components: {\n    MemebrRegister\n  }\n};","map":{"version":3,"mappings":"AAqCA,OAAOA,cAAP,MAA2B,iCAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,UAAT,EAAqBC,YAArB,EAAmCC,QAAnC,QAAmD,MAAnD;AAEA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,QAAQ,EAAC,EACP,GAAGF,QAAQ,CAAC,CAAC,aAAD,EAAe,YAAf,CAAD;EADJ,CAFE;;EAKXG,OAAO,GAAE;IACP,KAAKC,aAAL;EACD,CAPU;;EAQXC,OAAO,EAAC,EACN,GAAGN,YAAY,CAAC,CAAC,kBAAD,EAAoB,iBAApB,CAAD,CADT;IAEN,GAAGD,UAAU,CAAC,CAAC,eAAD,CAAD,CAFP;;IAINQ,YAAY,CAACC,EAAD,EAAI;MACdV,KAAK,CAACW,MAAN,CAAc,sBAAqBD,EAAG,EAAtC,EACCE,IADD,CACM,MAAI;QACR,KAAKL,aAAL;MACD,CAHD,EAICM,KAJD,CAIQC,GAAD,IAAO;QACZC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACD,CAND;IAOF;;EAZM,CARG;;EAsBXG,IAAI,GAAG;IACH,OAAO;MACLC,UAAU,EAAC;IADN,CAAP;EAGH,CA1BU;;EA4BXC,UAAU,EAAE;IAAEpB;EAAF;AA5BD,CAAf","names":["MemebrRegister","axios","mapActions","mapMutations","mapState","name","computed","created","getMemberList","methods","deleteMember","id","delete","then","catch","err","console","error","data","memberList","components"],"sourceRoot":"","sources":["C:\\Users\\J.H.Lee\\Documents\\ProjectFiles\\Inventory Management\\front\\src\\pages\\COOPmanagement\\CoopMember.vue"],"sourcesContent":["<template>\r\n\r\n<MemebrRegister v-if=\"memberModal==true\" @memberRegister=\"closeMemberModal();getMemberList()\"/>\r\n\r\n<div class=