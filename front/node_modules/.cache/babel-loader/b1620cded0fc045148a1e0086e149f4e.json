{"ast":null,"code":"import { mapActions, mapMutations, mapState } from 'vuex';\nimport InventoryRegister from '@/components/inventoryRegister.vue';\nimport axios from 'axios';\nexport default {\n  name: 'inventoryStatus',\n  components: {\n    InventoryRegister\n  },\n  computed: { ...mapState(['receivedList', 'inventoryModal', 'quantityList', 'getQuantity'])\n  },\n\n  created() {\n    this.getQuantity();\n    this.getReceived();\n  },\n\n  methods: { ...mapMutations(['openInventoryModal', 'closeInventoryModal']),\n    ...mapActions(['getReceived', 'productList']),\n\n    deleteInventory(id) {\n      axios.delete(`/api/inventory/delete/${id}`).then(() => {\n        this.getReceived();\n      }).catch(err => {\n        console.error(err);\n      });\n      this.getQuantity();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2DA,SAAUA,UAAV,EAAsBC,YAAtB,EAAoCC,QAApC,QAAoD,MAApD;AACA,OAAOC,iBAAP,MAA8B,oCAA9B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAc;EACVC,IAAI,EAAC,iBADK;EAEVC,UAAU,EAAC;IAAEH;EAAF,CAFD;EAGVI,QAAQ,EAAC,EACL,GAAGL,QAAQ,CAAC,CAAC,cAAD,EAAgB,gBAAhB,EAAiC,cAAjC,EAAgD,aAAhD,CAAD;EADN,CAHC;;EAMVM,OAAO,GAAE;IACL,KAAKC,WAAL;IACA,KAAKC,WAAL;EACH,CATS;;EAUVC,OAAO,EAAC,EACJ,GAAGV,YAAY,CAAC,CAAC,oBAAD,EAAsB,qBAAtB,CAAD,CADX;IAEJ,GAAGD,UAAU,CAAC,CAAC,aAAD,EAAe,aAAf,CAAD,CAFT;;IAKJY,eAAe,CAACC,EAAD,EAAI;MACfT,KAAK,CAACU,MAAN,CAAc,yBAAwBD,EAAG,EAAzC,EACCE,IADD,CACM,MAAI;QACN,KAAKL,WAAL;MACH,CAHD,EAICM,KAJD,CAIQC,GAAD,IAAO;QACVC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACH,CAND;MAOA,KAAKR,WAAL;IAEJ;;EAfI;AAVE,CAAd","names":["mapActions","mapMutations","mapState","InventoryRegister","axios","name","components","computed","created","getQuantity","getReceived","methods","deleteInventory","id","delete","then","catch","err","console","error"],"sourceRoot":"","sources":["C:\\Users\\J.H.Lee\\Documents\\ProjectFiles\\Inventory Management\\front\\src\\pages\\InventoryManagement\\InventoryStatus.vue"],"sourcesContent":["<template>\r\n\r\n<InventoryRegister v-if=\"inventoryModal == true\" @inventoryRegister=\"getReceived();getQuantity();closeInventoryModal();\"/>\r\n\r\n<div class=\"received-list\">\r\n    <table class=\"user-table\">\r\n        <caption>received list</caption>\r\n    <thead>\r\n        <th scope=\"col\">#</th>\r\n        <th scope=\"col\">Product</th>\r\n        <th scope=\"col\">Quantity</th>\r\n        <th scope=\"col\">Member name</th>\r\n        <th scope=\"col\">Price</th>\r\n        <th scope=\"col\">Quailty</th>\r\n        <th scope=\"col\">Receiving date</th>\r\n        <th scope=\"col\">delete</th>\r\n    </thead>\r\n    <tbody>\r\n        <tr scope=\"row\" class=\"inventoryList\" v-for=\"(item,i) in receivedList\" :key=\"item\">\r\n            <th scope=\"row\">{{i+1}}</th>\r\n            <td>{{item.Product_name}}</td>\r\n            <td>{{item.Warehousing_amount}}</td>\r\n            <td>{{item.Member_name}}</td>\r\n            <td>{{item.Price}}</td>\r\n            <td>{{item.Product_quailty}}</td>\r\n            <td>{{item.Warehousing_date.substr(0,10)}}</td>\r\n            <td><button type=\"button\" class=\"btn btn-light\" @click=\"deleteInventory(item.Warehousing_id)\">X</button></td>\r\n        </tr>\r\n    </tbody>\r\n</table>\r\n\r\n<button class=\"btn btn-primary\" @click=\"openInventoryModal()\">inventory Register</button>\r\n</div>\r\n\r\n<div class=\"inventory\">\r\n    <!-- 입고예정 수량 + 입고된 수량 -->\r\n    <table>\r\n        <caption>inventory</caption>\r\n        <thead>\r\n            <th scope=\"col\">#</th>\r\n            <th scope=\"col\">Product</th>\r\n            <th scope=\"col\">Current Quantity</th>\r\n            <th scope=\"col\">warehousing schedule Quantity</th>\r\n        </thead>\r\n        <tbody>\r\n            <tr scope=\"row\" v-for=\"(item,i) in quantityList\" :key=\"item\">\r\n                <th>{{i+1}}</th>\r\n                <td>{{item.Product_name}}</td>\r\n                <td>{{item.Warehousing_amount}}</td>\r\n                <td>{{item.Shipment_amount}}</td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n</div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport {  mapActions, mapMutations, mapState } from 'vuex'\r\nimport InventoryRegister from '@/components/inventoryRegister.vue'\r\nimport axios from 'axios'\r\nexport default{\r\n    name:'inventoryStatus',\r\n    components:{ InventoryRegister },\r\n    computed:{\r\n        ...mapState(['receivedList','inventoryModal','quantityList','getQuantity']),\r\n    },\r\n    created(){\r\n        this.getQuantity();\r\n        this.getReceived();\r\n    },\r\n    methods:{\r\n        ...mapMutations(['openInventoryModal','closeInventoryModal']),\r\n        ...mapActions(['getReceived','productList',]),\r\n        \r\n        \r\n        deleteInventory(id){\r\n            axios.delete(`/api/inventory/delete/${id}`)\r\n            .then(()=>{\r\n                this.getReceived();\r\n            })\r\n            .catch((err)=>{\r\n                console.error(err)\r\n            })\r\n            this.getQuantity();\r\n\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.inventory{\r\n    float: left;\r\n    height: 100%;\r\n    width:50%\r\n}\r\n.received-list{\r\n    float: left;\r\n    height: 100%;\r\n    width:50%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}