{"ast":null,"code":"import axios from 'axios';\nimport LineChart from '@/components/Chart.vue';\nexport default {\n  name: \"monitoringSimulation\",\n  components: {\n    LineChart\n  },\n\n  data() {\n    return {\n      storagePeriod: null,\n      demand: null,\n      supply: null,\n      engineGo: true,\n      GoData: null,\n      chartData: {\n        labels: ['4 / 8', '4 / 9', '4 / 10', '4 / 11', '4 / 12', '4 / 13', '4 / 14', '4 / 15', '4 / 16', '4 / 17'],\n        datasets: [{\n          borderWidth: 1,\n          // 라인 넓이\n          label: '총 수면 시간',\n          // 데이터 라벨\n          backgroundColor: 'rgb(255,255,255, 0)',\n          pointBackgroundColor: '#64CCA2',\n          fill: true,\n          // 채우기\n          // barPercentage: 1, 바 안쪽 넓이\n          // categoryPercentage: 0.5, 바 외부 넓이\n          tension: 0.1,\n          borderColor: '#64CCA2',\n          pointBorderColor: '#64CCA2',\n          pointBorderWidth: 1,\n          data: [45, 50, 60, 50, 45, 80, 50, 40, 68, 70]\n        }]\n      },\n      options: {\n        responsive: true,\n        maintainAspectRatio: false\n      }\n    };\n  },\n\n  methods: {\n    simulationStart() {\n      const period = this.storagePeriod;\n      const demand = this.demand;\n      const supply = this.supply;\n      axios.post(`/api/simulate/engine`, {\n        period,\n        demand,\n        supply\n      }).then(res => {\n        this.GoData = res.data;\n      }).catch(err => {\n        console.error(err);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,SAAP,MAAsB,wBAAtB;AAEA,eAAe;EACXC,IAAI,EAAE,sBADK;EAEXC,UAAU,EAAE;IAAEF;EAAF,CAFD;;EAGXG,IAAI,GAAG;IACH,OAAO;MACHC,aAAa,EAAE,IADZ;MAEHC,MAAM,EAAE,IAFL;MAGHC,MAAM,EAAE,IAHL;MAIHC,QAAQ,EAAE,IAJP;MAKHC,MAAM,EAAE,IALL;MAMHC,SAAS,EAAE;QACPC,MAAM,EAAE,CAAC,OAAD,EAAU,OAAV,EAAmB,QAAnB,EAA6B,QAA7B,EAAuC,QAAvC,EAAiD,QAAjD,EAA2D,QAA3D,EAAqE,QAArE,EAA+E,QAA/E,EAAyF,QAAzF,CADD;QAEPC,QAAQ,EAAE,CACN;UACIC,WAAW,EAAE,CADjB;UACoB;U