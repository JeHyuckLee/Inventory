dalState']),\r\n\r\n    checkForm(e){\r\n      if(this.name && this.id && this.address && this.phone && this.password){\r\n        this.name=null;\r\n        this.id = null;\r\n        this.address = null;\r\n        this.phone=null;\r\n        this.password = null;\r\n        return true;\r\n      }\r\n      this.errors = [];\r\n\r\n      if(!this.name){\r\n        this.errors.push('Name required');\r\n      }\r\n      if(!this.id){\r\n        this.errors.push('id require');\r\n      }\r\n      if(!this.address){\r\n        this.errors.push('address require');\r\n      }\r\n      // if(!this.phone){\r\n      //   this.errors.push('id require');\r\n      // }\r\n      if(!this.password){\r\n        this.errors.push('password require');\r\n      }\r\n      e.preventDefault();\r\n      alert('can not submit');\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.black-bg{\r\n  width:100%;\r\n  height: 100%;\r\n  background: rgba(0,0,0,0.5);\r\n  position:fixed;\r\n  padding:20px;\r\n\r\n\r\n}\r\n.white-bg{\r\n  width:100%;\r\n  background:white;\r\n  border-radius: 8px;\r\n  padding:20px;\r\n\r\n}\r\n\r\n\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}