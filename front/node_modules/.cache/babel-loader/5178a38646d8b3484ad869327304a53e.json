\n            return true;\r\n          }\r\n          this.errors = [];\r\n    \r\n          if(!this.product){\r\n            this.errors.push('Product required');\r\n          }\r\n    \r\n          if(!this.date){\r\n            this.errors.push('date require');\r\n          }\r\n          // if(!this.phone){\r\n          //   this.errors.push('id require');\r\n          // }\r\n          if(!this.amount){\r\n            this.errors.push('amount require');\r\n          }\r\n          if(!this.price){\r\n            this.errors.push('price require');\r\n          }\r\n          \r\n          alert(this.errors);\r\n    \r\n        },\r\n      }\r\n    }\r\n    </script>\r\n    \r\n    <style>\r\n    \r\n    \r\n    </style>"]},"metadata":{},"sourceType":"module"}