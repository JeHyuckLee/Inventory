{"ast":null,"code":"import MemebrRegister from '@/components/MemberRegister.vue';\nimport axios from 'axios';\nimport { mapMutations, mapState } from 'vuex';\nexport default {\n  name: \"coopRegister\",\n  computed: { ...mapState(['registerModal'])\n  },\n\n  created() {\n    this.getMemberList();\n  },\n\n  methods: { ...mapMutations(['closeRegisterModal', 'openRegisterModal']),\n\n    getMemberList() {\n      axios.get('/api/member/data').then(res => {\n        this.memberList = res.data;\n      }).catch(err => {\n        console.error(err);\n      });\n    },\n\n    deleteMember(id) {\n      axios.delete(`/api/member/delete/${id}`).then(() => {\n        this.getMemberList();\n      }).catch(err => {\n        console.error(err);\n      });\n    }\n\n  },\n\n  data() {\n    return {\n      memberList: []\n    };\n  },\n\n  components: {\n    MemebrRegister\n  }\n};","map":{"version":3,"mappings":"AAuCA,OAAOA,cAAP,MAA2B,iCAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,YAAT,EAAuBC,QAAvB,QAAuC,MAAvC;AAEA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,QAAQ,EAAC,EACP,GAAGF,QAAQ,CAAC,CAAC,eAAD,CAAD;EADJ,CAFE;;EAKXG,OAAO,GAAE;IACP,KAAKC,aAAL;EACD,CAPU;;EAQXC,OAAO,EAAC,EACN,GAAGN,YAAY,CAAC,CAAC,oBAAD,EAAsB,mBAAtB,CAAD,CADT;;IAGNK,aAAa,GAAE;MACbN,KAAK,CAACQ,GAAN,CAAU,kBAAV,EACDC,IADC,CACKC,GAAD,IAAO;QACX,KAAKC,UAAL,GAAkBD,GAAG,CAACE,IAAtB;MACD,CAHC,EAIDC,KAJC,CAIMC,GAAD,IAAO;QACZC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACD,CANC;IAOD,CAXK;;IAaNG,YAAY,CAACC,EAAD,EAAI;MACdlB,KAAK,CAACmB,MAAN,CAAc,sBAAqBD,EAAG,EAAtC,EACCT,IADD,CACM,MAAI;QACR,KAAKH,aAAL;MACD,CAHD,EAICO,KAJD,CAIQC,GAAD,IAAO;QACZC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACD,CAND;IAOF;;EArBM,CARG;;EA+BXF,IAAI,GAAG;IACH,OAAO;MACLD,UAAU,EAAC;IADN,CAAP;EAGH,CAnCU;;EAqCXS,UAAU,EAAE;IAAErB;EAAF;AArCD,CAAf","names":["MemebrRegister","axios","mapMutations","mapState","name","computed","created","getMemberList","methods","get","then","res","memberList","data","catch","err","console","error","deleteMember","id","delete","components"],"sourceRoot":"","sources":["C: