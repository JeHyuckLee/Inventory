{"ast":null,"code":"import ProductRegister from '@/components/ProductRegister.vue';\nimport axios from 'axios';\nimport { mapActions, mapMutations, mapState } from 'vuex';\nexport default {\n  name: 'inventoryProduct',\n  computed: { ...mapState(['productList', 'productModal'])\n  },\n\n  created() {\n    this.getProductList();\n  },\n\n  methods: { ...mapMutations(['closeProductModal', 'openProductModal']),\n    ...mapActions(['getProductList']),\n\n    deleteProduct(id) {\n      axios.delete(`/api/product/delete/${id}`).then(() => {\n        this.getProductList();\n      }).catch(err => {\n        console.error(err);\n      });\n    }\n\n  },\n  components: {\n    ProductRegister\n  }\n};","map":{"version":3,"mappings":"AAwCA,OAAOA,eAAP,MAA4B,kCAA5B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,UAAT,EAAqBC,YAArB,EAAmCC,QAAnC,QAAmD,MAAnD;AACA,eAAe;EACXC,IAAI,EAAC,kBADM;EAEXC,QAAQ,EAAC,EACP,GAAGF,QAAQ,CAAC,CAAC,aAAD,EAAe,cAAf,CAAD;EADJ,CAFE;;EAKXG,OAAO,GAAE;IACP,KAAKC,cAAL;EACD,CAPU;;EAQXC,OAAO,EAAC,EACN,GAAGN,YAAY,CAAC,CAAC,mBAAD,EAAqB,kBAArB,CAAD,CADT;IAEN,GAAGD,UAAU,CAAC,CAAC,gBAAD,CAAD,CAFP;;IAGNQ,aAAa,CAACC,EAAD,EAAI;MACfV,KAAK,CAACW,MAAN,CAAc,uBAAsBD,EAAG,EAAvC,EACCE,IADD,CACM,MAAI;QACR,KAAKL,cAAL;MACD,CAHD,EAICM,KAJD,CAIQC,GAAD,IAAO;QACZC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACD,CAND;IAOD;;EAXK,CARG;EAsBXG,UAAU,EAAC;IAAClB;EAAD;AAtBA,CAAf","names":["ProductRegister","axios","mapActions","mapMutations","mapState","name","computed","created","getProductList","methods","deleteProduct","id","delete","then","catch","err","console","error","components"],"sourceRoot":"","sources":["C:\\Users\\J.H.Lee\\Documents\\ProjectFiles\\Inventory Management\\front\\src\\pages\\InventoryManagement\\InventoryProduct.vue"],"sourcesContent":["<template>\r\n<ProductRegister v-if=\"productModal == true\" @productRegist=\"closeProductModal();getProductList()\"/>\r\n\r\n<div class=\"register\">\r\n  <table class=\"user-table\">\r\n    <caption>List of Product</caption>\r\n    <thead >\r\n      <tr>\r\n        <th scope=\"col\">#</th>\r\n        <th scope=\"col\">Name</th>\r\n        <th scope=\"col\">Category</th>\r\n        <th scope=\"col\">Unit</th>\r\n        <th scope=\"col\">Price</th>\r\n        <th scope=\"col\">Delete</th>\r\n      </tr>\r\n    </thead>\r\n    <tbody>\r\n      <tr class=\"memberList\" scope=\"row\" v-for=\"(item,i) in productList\" :key=\"item\" >\r\n        <th scope=\"row\">{{i+1}}</th>\r\n        <td>{{item.Product_name}}</td>\r\n        <td>{{item.Product_category}}</td>\r\n        <td>{{item.Product_unit}}</td>\r\n        <td>{{item.Product_price}}</td>\r\n        <td><button type=\"button\" class=\"btn btn-light\" @click=\"deleteProduct(item.Product_id)\">X</button></td>\r\n      </tr>\r\n    </tbody>\r\n  </table>\r\n\r\n  <div>\r\n    <button type=\"button\" class=\"btn btn-primary\" @click=\"openProductModal()\">\r\n    New Product\r\n    </button>\r\n  </div>\r\n</div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport ProductRegister from '@/components/ProductRegister.vue';\r\nimport axios from 'axios'\r\nimport { mapActions, mapMutations, mapState } from 'vuex'\r\nexport default {\r\n    name:'inventoryProduct',\r\n    computed:{\r\n      ...mapState(['productList','productModal']),\r\n    },\r\n    created(){\r\n      this.getProductList();\r\n    },\r\n    methods:{\r\n      ...mapMutations(['closeProductModal','openProductModal']),\r\n      ...mapActions(['getProductList']),\r\n      deleteProduct(id){\r\n        axios.delete(`/api/product/delete/${id}`)\r\n        .then(()=>{\r\n          this.getProductList();\r\n        })\r\n        .catch((err)=>{\r\n          console.error(err);\r\n        })\r\n      },\r\n    \r\n    },\r\n    components:{ProductRegister},\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}