{"ast":null,"code":"import MemebrRegister from '@/components/MemberRegister.vue';\nimport axios from 'axios';\nimport { mapState } from 'vuex';\nexport default {\n  name: \"coopRegister\",\n  computed: { ...mapState(['registerModal'])\n  },\n\n  created() {\n    this.getMemberList();\n  },\n\n  methods: {\n    getMemberList() {\n      axios.get('/api/member/data').then(res => {\n        this.memberList = res.data;\n      }).catch(err => {\n        console.error(err);\n      });\n    },\n\n    deleteMember(id) {\n      axios.delete(`/api/member/delete/:${id}`).then(res => {\n        console.log(res);\n        this.getMemberList();\n      }).catch(err => {\n        console.error(err);\n      });\n    }\n\n  },\n\n  data() {\n    return {\n      memberList: []\n    };\n  },\n\n  components: {\n    MemebrRegister\n  }\n};","map":{"version":3,"mappings":"AAuCA,OAAOA,cAAP,MAA2B,iCAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,QAAyB,MAAzB;AAEA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,QAAQ,EAAC,EACP,GAAGF,QAAQ,CAAC,CAAC,eAAD,CAAD;EADJ,CAFE;;EAKXG,OAAO,GAAE;IACP,KAAKC,aAAL;EACD,CAPU;;EAQXC,OAAO,EAAC;IACND,aAAa,GAAE;MACbL,KAAK,CAACO,GAAN,CAAU,kBAAV,EACDC,IADC,CACKC,GAAD,IAAO;QACX,KAAKC,UAAL,GAAkBD,GAAG,CAACE,IAAtB;MAED,CAJC,EAKDC,KALC,CAKMC,GAAD,IAAO;QACZC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACD,CAPC;IAQD,CAVK;;IAYNG,YAAY,CAACC,EAAD,EAAI;MACdjB,KAAK,CAACkB,MAAN,CAAc,uBAAsBD,EAAG,EAAvC,EACCT,IADD,CACOC,GAAD,IAAO;QACXK,OAAO,CAACK,GAAR,CAAYV,GAAZ;QACA,KAAKJ,aAAL;MACD,CAJD,EAKCO,KALD,CAKQC,GAAD,IAAO;QACZC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACD,CAPD;IAQF;;EArBM,CARG;;EA+BXF,IAAI,GAAG;IACH,OAAO;MACLD,UAAU,EAAC;IADN,CAAP;EAGH,CAnCU;;EAqCXU,UAAU,EAAE;IAAErB;EAAF;AArCD,CAAf","names":["MemebrRegister","axios","mapState","name","computed","created","getMemberList","methods","get","then","res","memberList","data","catch","err","console","error","deleteMember","id","delete","log","components"],"sourceRoot":"","sources":["C:\\Users\\J.H.Lee\\Documents\\ProjectFiles\\Inventory Management\\front\\src\\pages\\COOP