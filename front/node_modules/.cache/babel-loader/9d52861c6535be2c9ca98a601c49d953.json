{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { mapMutations, mapState } from 'vuex';\nexport default {\n  name: \"salesRegister\",\n  computed: { ...mapState(['salesModal'])\n  },\n\n  data() {\n    return {\n      errors: [],\n      name: null,\n      date: null,\n      amount: null,\n      price: null\n    };\n  },\n\n  methods: { ...mapMutations(['closeSalesModal']),\n\n    sendSalesData() {\n      axios.post('/api/sales/data', {\n        name: this.name,\n        date: this.date,\n        amount: this.amount,\n        price: this.price\n      }).then(() => {\n        this.$emit('SalesRegister');\n      }).catch(err => {\n        console.error(err);\n      });\n    },\n\n    registerSales() {\n      if (this.name && this.date && this.amount && this.price) {\n        this.sendSalesData();\n        return true;\n      }\n\n      this.errors = [];\n\n      if (!this.name) {\n        this.errors.push('Name required');\n      }\n\n      if (!this.date) {\n        this.errors.push('date require');\n      } // if(!this.phone){\n      //   this.errors.push('id require');\n      // }\n\n\n      if (!this.amount) {\n        this.errors.push('amount require');\n      }\n\n      if (!this.price) {\n        this.errors.push('price require');\n      }\n\n      alert(this.errors);\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA4CI,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,YAAT,EAAuBC,QAAvB,QAAuC,MAAvC;AAGA,eAAe;EACbC,IAAI,EAAC,eADQ;EAEbC,QAAQ,EAAC,EACL,GAAGF,QAAQ,CAAC,CAAC,YAAD,CAAD;EADN,CAFI;;EAMbG,IAAI,GAAE;IACJ,OAAM;MACJC,MAAM,EAAC,EADH;MAEJH,IAAI,EAAC,IAFD;MAGJI,IAAI,EAAC,IAHD;MAIJC,MAAM,EAAC,IAJH;MAKJC,KAAK,EAAC;IALF,CAAN;EAOD,CAdY;;EAgBbC,OAAO,EAAC,EACN,GAAGT,YAAY,CAAC,CAAC,iBAAD,CAAD,CADT;;IAGNU,aAAa,GAAE;MACbX,KAAK,CAACY,IAAN,CAAW,iBAAX,EAA6B;QAC3BT,IAAI,EAAC,KAAKA,IADiB;QAE3BI,IAAI,EAAC,KAAKA,IAFiB;QAG3BC,MAAM,EAAC,KAAKA,MAHe;QAI3BC,KAAK,EAAC,KAAKA;MAJgB,CAA7B,EAMCI,IAND,CAMM,MAAI;QACR,KAAKC,KAAL,CAAW,eAAX;MACD,CARD